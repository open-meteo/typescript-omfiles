{
  "name": "@openmeteo/file-reader",
  "version": "0.0.15",
  "description": "JavaScript reader for the om file format using WebAssembly",
  "repository": {
    "type": "git",
    "url": "https://github.com/open-meteo/typescript-omfiles.git",
    "directory": "packages/file-reader"
  },
  "type": "module",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "types": "./dist/index.d.ts",
    "import": {
      "browser": "./dist/esm/index.browser.js",
      "default": "./dist/esm/index.js"
    },
    "require": {
      "browser": "./dist/cjs/index.browser.cjs",
      "default": "./dist/cjs/index.cjs"
    }
  },
  "scripts": {
    "build": "rollup -c",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/node": "^24.10.10",
    "@vitest/coverage-v8": "^4.0.18",
    "rollup": "^4.57.1",
    "rollup-plugin-dts": "^6.3.0",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.5"
  },
  "dependencies": {
    "@openmeteo/file-format-wasm": "^0.0.15"
  },
  "files": [
    "dist/**/*"
  ],
  "keywords": [
    "open-meteo",
    "meteorological-data"
  ],
  "license": "GPL-2.0-only"
}
